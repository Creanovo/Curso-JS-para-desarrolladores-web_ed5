// paso 2

  function clientes (nombre, usuario, password, tipoUsuario, presupuesto) {
        clientes.nombre = nombre;
        clientes.usuario = usuario;
        clientes.password = password;
        clientes.tipoUsario = tipoUsuario;
        clientes.presupuesto = presupuesto;
        }
    
